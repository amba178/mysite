- if object.errors.any?
	.error_explanation
		%div{class: "alert alert-danger error-explanations" }
			The form contains 
			= pluralize(object.errors.count, 'error')
		
			%ul
				= list_of object.errors.full_messages.each do |msg|
					- if msg.class==Hash
						#{msg.fetch(:message, "Something went wrong!")}
					- else 
						#{msg}
